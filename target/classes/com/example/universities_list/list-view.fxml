          <padding>
              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
          </padding>
         <children>
            <Label id="label" stylesheets="@style.css" text="Search for Universities" />
            <HBox maxWidth="400.0" spacing="5.0">
               <children>
                  <TextField prefHeight="35.0" prefWidth="636.0" />
                  <Button mnemonicParsing="false" prefHeight="34.0" prefWidth="378.0" text="Search" />
               </children>
            </HBox>
            <ListView maxWidth="500.0" prefHeight="376.0" prefWidth="561.0" style="-fx-background-radius: 10px;" />
